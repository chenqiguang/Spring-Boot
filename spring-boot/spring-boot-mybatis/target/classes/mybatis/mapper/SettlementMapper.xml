<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chenqiguang.springboot.mybatis.mapper.SettlementMapper">
    <select id="getSettlementById" resultType="com.chenqiguang.springboot.mybatis.bean.Settlement">
        select * from chenqiguang.t_settlement  where settlement_Id = #{settlementId}
    </select>
</mapper>
